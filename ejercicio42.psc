Proceso ejercicio42
	//Tabla de multiplicar
	//entrada
	Definir num, i como Entero;
    Escribir "ingrese un numero para ubicar que tabla de multiplicar nesecita ";
    Leer num;
    Escribir "Tabla de multiplicar del ", num, " del 1 al 12:";
	//Proceso 
    Para i <- 1 Hasta 12 Con Paso 1 Hacer
        Escribir num, " x ", i, " = ", num * i;
    FinPara
	//salida
FinProceso
